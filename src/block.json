{
	"change": ["name", "title", "textdomain"],
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "block-template/dynamic-block",
	"version": "0.1.0",
	"title": "Dynamic Block",
	"category": "text",
	"icon": "smiley",
	"description": "Display and filter latest posts",
	"keywords": ["posts", "latest"],
	"supports": {
		"html": false
	},
	"textdomain": "dynamic-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"numberOfPosts": {
			"type": "number",
			"default": 5
		},
		"displayFeaturedImage": {
			"type": "boolean",
			"default": true
		},
		"order": {
			"type": "string",
			"default": "desc"
		},
		"orderBy": {
			"type": "string",
			"default": "date"
		}
	}
}